  "amexio-dropdown-basic":
    prefix: "amexio-dropdown-basic"
    body: """
      <amexio-dropdown [(ngModel)]=\"${1:ngModel}\"
      \t [placeholder]=\"'${2:placeholder}'\"name=\"${3:name}\"
      \t [dataReader]=\"'response.data'\"
      \t [fieldLabel]=\"'${5:fieldLabel}'\"
      \t [httpUrl]=\"'${6:URL}'\"
      \t [httpMethod]=\"'${7:httpMethod}'\"
      \t [displayField]=\"'${8:displayField}'\"
      \t [valueField]=\"'${9:valueField}'\"
      \t [width]=\"'${10:width}'\"
      \t (onSingleSelect)=\"getSingleVal($${11:event})\"></amexio-dropdown>
      \t <div class=\"well well-sm\">Selected Value: "
      <b>{{${12:selectedValue}}}</b></div>
      \t <div> <pre> {{${13:selectedObject}}}</pre></div>
      """
    description: "Amexio Dropdown"
  "amexio-dropdown-filter":
    prefix: "amexio-dropdown-filter"
    body: """
      <amexio-dropdown [(ngModel)]=\"${1:ngModel}\"
      \t [placeholder]=\"'${2:placeholder}'\" name=\"${3:name}\"
      \t [dataReader]=\"'response.data'\"
      \t [fieldLabel]=\"'${4:fieldLabel}'\"
      \t [httpUrl]=\"'${5:httpUrl}'\"
      \t [httpMethod]=\"'${6:httpMethod}'\"
      \t [searchBox]=\"'${7:searchBox}'\"
      \t [displayField]=\"'${8:displayField}'\"
      \t [valueField]=\"'${9:valueField}'\"
      \t [width]=\"'auto'\"></amexio-dropdown>
      \t <div class=\"well well-sm\">Selected Value: "
      <b>{{${10:selectedValue}}}</b></div>
      """
    description: "Amexio Dropdown with Filter"
  "amexio-dropdown-multiselect":
    prefix: "amexio-dropdown-multiselect"
    body: """
      <amexio-dropdown  (onMultiSelect)=\"setMultiVal($${1:event})\"
      \t[placeholder]=\"'${2:placeholder}'\"
      \t[dataReader]=\"'response.data'\"
      \t[fieldLabel]=\"'${3:fieldLabel}'\"
      \t[httpUrl]=\"'${4:httpUrl}'\"
      \t[httpMethod]=\"'${5:httpMethod}'\"
      \t[displayField]=\"'${6:displayField}'\"
      \t[valueField]=\"'${7:valueField}'\"
      \t[width]=\"'auto'\"
      \t[multiSelect]=\"'true'\"></amexio-dropdown>
      \t<div class=\"panel-heading\"><b>Selected Value</b></div>
      \t<div class=\"panel-body\">
      \t<div *ngFor=\"let ft of fruit\"><p>
      \t{{${8:selectedValue}}}
      \t</p></div></div>
      """
    description: "Amexio Dropdown with Multi Select"
  "amexio-dropdown-multiselect-limited":
