    body: """
      <amexio-btn [label]=\"'${1:label}'\"
      \t [type]=\"'primary'\"
      \t [tooltipMessage]=\"'${2:tooltipMessage}'\"
      \t  [disabled]=\"'${3:disabled}'\">
      \t </amexio-btn>
      """
    description: "Amexio State Button"
  "amexio-async-button":
    prefix: "amexio-async-button"
    body: """
      <amexio-btn [label]=\"'${1:label}'\"
      \t [type]=\"'primary'\"
      \t [tooltipMessage]=\"'${3:tooltipMessage}'\"
      \t (onClick)=\"'${4:onClickMethod}'\"
      \t [isLoading]=\"'${5:asyncFlag}'\">
