      <amexio-btn-group>
      \t <amexio-btn-group-action [label]=\"'${1:label1}'\"
      [type]=\"'primary'\"
      \t  [tooltipMessage]=\"'Disabled'\" [disabled]=\"'true'\">
      </amexio-btn-group-action>
      \t <amexio-btn-group-action [label]=\"'${2:label2}'\"
      [type]=\"'primary'\"
      \t [tooltipMessage]=\"'Enabled'\"></amexio-btn-group-action>
      </amexio-btn-group>
