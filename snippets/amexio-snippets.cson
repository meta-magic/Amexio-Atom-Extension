       <amexio-card-pane [showCard]=\"true\">
        \t<amexio-pane-header> ${1:header}</amexio-pane-header>
        \t<amexio-pane-body>
        \t\t<div class=\"row\">
        \t\t\t<div class=\"col-lg-12\">
