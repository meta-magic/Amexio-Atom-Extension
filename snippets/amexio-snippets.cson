  "amexio-grid":
    prefix: "amexio-grid"
    body: """
      <amexio-data-table
      \t[title]=\"'${1:title}'\"
      \t[pageSize] =\"'${2:pageSize}'\"
      \t[dataReader]=\"'response.data'\"
      \t[checkboxSelect]=\"'${3:checkboxSelect}'\"
      \t[httpUrl]=\"'${4:httpURL}'\"
      \t[httpMethod]=\"'${5:httpMethod}'\"
      \t(selectedRowData)=\"onRowSelect($${6:event})\"
      \t(rowSelect)=\"onRowClick($${7:event})\">
      \t\t<amexio-data-table-column [dataIndex]=\"'${8:dataIndex}'\"
      \t\t [dataType]=\"'${9:dataType}'\" [hidden]=\"'${10:hidden}'\"
      \t\t [text]=\"'${11:text}'\"></amexio-data-table-column>
      \t\t<amexio-data-table-column [dataIndex]=\"'${12:dataIndex}'\"
      \t\t [dataType]=\"'${13:dataType2}'\" [hidden]=\"'${14:hidden2}'\"
      \t\t [text]=\"'${15:text2}'\"></amexio-data-table-column>
      \t\t <amexio-data-table-column [dataIndex]=\"'${16:dataIndex3}'\"
      \t\t [dataType]=\"'${17:dataType3}'\" [hidden]=\"'${18:hidden3}'\"
      \t\t [text]=\"'${19:text}'\" [summaryCaption]=\"'${20:summaryCaption}'\"
      \t [summaryType]=\"'${21:summaryType}'\"></amexio-data-table-column>
      </amexio-data-table>
      """
    description: "Amexio Data Grid"
  "amexio-grid-with-templates":
    prefix: "amexio-grid-with-templates"
    body: """
      <amexio-data-table \t[title]=\"'${1:title}'\"
      \t[pageSize] =\"'${2:pageSize}'\"
      \t[dataReader]=\"'response.data'\"
      \t[checkboxSelect]=\"'${3:checkboxSelect}'\"
      \t[httpUrl]=\"'${4:httpURL}'\"
      \t[httpMethod]=\"'${5:httpMethod}'\"
      \t(selectedRowData)=\"onRowSelect($${6:event})\"
      \t(rowSelect)=\"onRowClick($${7:event})\">
      \t<amexio-data-table-column [dataIndex]=\"'${8:dataIndex}'\"
      \t\t[dataType]=\"'${9:dataIndex}'\" [hidden]=\"'${10:hidden}'\"
      \t\t[text]=\"'${11:text}'\">
      \t\t<ng-template #amexioBodyTmpl let-column let-row=\"row\">
      \t\t\t<span>
      \t\t\t<img src=\"\" [attr.src]=\"'${14:imgSrc}'\"+"
      "row.flag_32\">&nbsp;{{row.name}} ( {{row.name_official}} )"
      \t\t\t</span>
      \t\t</ng-template>
      \t\t</amexio-data-table-column>
      \t\t<amexio-data-table-column
      \t\t\t[dataIndex]=\"'${15:dataIndex1}'\"
      \t\t [dataType]=\"'${16:dataType1}'\" [hidden]=\"'${17:hidden1}'\"
      \t\t[text]=\"'${18:text1}'\"></amexio-data-table-column>
      \t\t<amexio-data-table-column
      \t\t\t[dataIndex]=\"'${19:dataIndex2}'\"
      \t\t [dataType]=\"'${20:dataType2}'\" [hidden]=\"'${21:hidden2}'\"
      \t\t[text]=\"'${22:text2}'\"></amexio-data-table-column>
      \t\t</amexio-data-table>
      """
    description: "Amexio Data Grid with Templates"
  "amexio-tree-grid":
